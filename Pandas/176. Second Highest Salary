import pandas as pd

def second_highest_salary(employee: pd.DataFrame) -> pd.DataFrame:
    unique_salaries = employee["salary"].drop_duplicates()

    if (employee["salary"].nunique() == 1) | (employee["salary"].nunique() <1):
        data = {"secondHighestSalary": [None]}
        df = pd.DataFrame(data)
    elif employee["salary"].nunique() > 1:
        result = unique_salaries.sort_values(ascending = False)
        value =  result.iloc[1]
        data = {"SecondHighestSalary": [value]}
        df = pd.DataFrame(data)
    return df 
